<body>
    <header>
        <a href="#" ><img src="../multimedia/logo_transparent.png" class="logo"></a>
        <div class="toggle" onclick="toggleMenu();"></div>
        <ul class="navigation">
            <li><a asp-area="" asp-controller="RealityFirst" asp-action="Index">INICIO</a></li>
            <li><a asp-area="" asp-controller="RealityFirst" asp-action="Entradas">ENTRADAS</a></li>
            <li><a asp-area="" asp-controller="RealityFirst" asp-action="Noticias">NOTICIAS</a></li>
            <li><a asp-area="" asp-controller="RealityFirst" asp-action="Artistas">ARTISTAS</a></li> 
            <li><a asp-area="" asp-controller="RealityFirst" asp-action="Contacto">CONTACTO</a></li>
            <li><a asp-area="" asp-controller="RealityFirst" asp-action="Ingresar">INGRESAR</a></li>
          </ul>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Pruebas - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    
<script type="text/javascript">
  function imgSlider(anything){
    document.querySelector('.Starbucks').src = anything; 
  }
  function changeCircleColor(color){
    const circle = document.querySelector('.circle');
    circle.style.background = color;
  }

  function toggleMenu(){
    var menutoggle = document.querySelector('.toggle');
    var navigation = document.querySelector('.navigation')
    menutoggle.classList.toggle('active')
    navigation.classList.toggle('active')
  }
</script>
